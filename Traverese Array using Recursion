public class Main{
    public static void Traverse(int a[],int n,int i){
        if(i>=n){
            return;
        }
        else{
            System.out.print(a[i]+" ");
            Traverse(a,n,i+1);
        }
    }
    public static void main(String[] args){
        int [] a={8,2,9,4,10};
        Traverse(a,5,0);
        
    }
}
