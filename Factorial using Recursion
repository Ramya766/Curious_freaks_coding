import java.util.Scanner;
public class Main{
public static int Fact(int n){
    if(n==1){
        return 1;
    }
    else{
           return n*Fact(n-1);
       
    }
}

    public static void main(String[] args){
        Scanner S=new Scanner(System.in);
        int n=S.nextInt();
        Fact(n);
        int res=Fact(n);
        System.out.print(res);
        
        
        
        
    }
}
